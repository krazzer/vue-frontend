{
  "name": "cms-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open --host kikcms2.test",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "preview": "vite preview --port 5050",
    "test": "npm run test:unit && npm run build && npm run test:e2e",
    "test:unit": "vitest --environment jsdom",
    "test:unit:coverage": "vitest --environment jsdom run --coverage",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/",
    "cypress:open": "cypress open",
    "cypress": "cypress run",
    "test:e2e:ci": "npm run build && start-server-and-test preview http://localhost:5050/cms/ cypress",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev:datatable": "vite --config vite.datatable.config.ts",
    "build:datatable": "vue-tsc --noEmit && vite --config vite.datatable.config.ts build --emptyOutDir -- --target wc --name kikcms-datatable",
    "preview:datatable": "vite --config vite.datatable.config.ts preview --port 5051"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@tinymce/tinymce-vue": "^5.1.1",
    "@types/node": "^20.4.8",
    "@volar/language-server": "2.4.0-alpha.18",
    "@vue/language-server": "2.0.28",
    "@vue/web-component-wrapper": "^1.3.0",
    "axios": "^1.7.9",
    "vue": "^3.5.13",
    "vue-axios": "^3.4.1",
    "vue-inline-svg": "^3.1.0",
    "vue-router": "^4.0.12",
    "vuetify": "^3.3.9"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jest": "^29.5.3",
    "@types/jsdom": "^21.1.1",
    "@types/mocha": "^10.0.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vitest/coverage-v8": "^0.33.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "axios-mock-adapter": "^1.20.0",
    "c8": "^8.0.0",
    "cypress": "^12.17.2",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.15.1",
    "flush-promises": "^1.0.2",
    "jest": "^29.6.1",
    "jsdom": "^22.1.0",
    "msw": "^1.2.3",
    "prettier": "^3.0.0",
    "sass": "^1.64.1",
    "scss": "^0.2.4",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.7.2",
    "vite": "^4.4.7",
    "vitest": "^0.33.0",
    "vue-tsc": "^2.1.10"
  }
}
