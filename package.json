{
  "name": "cms-frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open --host kikcms.test",
    "devNoMock": "VITE_API_HOST=https://localhost:9201/ vite --open --host kikcms.test",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "buildToBackend": "rm -rf ../cms-backend/public_html/cms/* && vue-tsc --noEmit --skipLibCheck && BUILD_OUTDIR=../cms-backend/public_html/cms/ vite build",
    "preview": "vite preview --port 5050",
    "test": "npm run test:unit && npm run build && npm run test:e2e",
    "test:unit": "vitest --environment jsdom run",
    "test:unit:watch": "vitest --environment jsdom",
    "test:unit:coverage": "vitest --environment jsdom run --coverage && open coverage/index.html",
    "test:e2e": "npm run cypress",
    "test:e2e:coverage": "VITE_COVERAGE=true npm run cypress && npm run coverage:open",
    "cypress:open": "cypress open",
    "cypress": "NODE_NO_WARNINGS=1 cypress run",
    "coverage:report": "nyc report --reporter=html",
    "coverage:open": "open coverage/index.html",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev:datatable": "vite --config vite.datatable.config.ts",
    "build:datatable": "vue-tsc --noEmit && vite --config vite.datatable.config.ts build --emptyOutDir -- --target wc --name kikcms-datatable",
    "preview:datatable": "vite --config vite.datatable.config.ts preview --port 5051"
  },
  "nyc": {
    "exclude": [
      "**/mock.ts",
      "**/mockSorter.ts",
      "**/mocker.ts",
      "**/icons/*"
    ],
    "extension": [".ts", ".vue"],
    "reporter": ["text", "html"],
    "sourceMap": true,
    "instrument": false
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@tinymce/tinymce-vue": "^6.3.0",
    "@types/node": "^25.0.1",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "@volar/language-server": "2.4.27",
    "@vue/language-server": "^3.1.8",
    "@vue/web-component-wrapper": "^1.3.0",
    "axios": "^1.13.2",
    "ts-node": "^10.9.2",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue": "^3.5.25",
    "vue-axios": "^3.5.2",
    "vue-inline-svg": "^4.0.1",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.7",
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/mocha": "^10.0.10",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "axios-mock-adapter": "^2.1.0",
    "c8": "^10.1.3",
    "cypress": "^15.7.1",
    "eslint": "^9.39.1",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-vue": "^10.6.2",
    "flush-promises": "^1.0.2",
    "jest": "^30.2.0",
    "jsdom": "^27.3.0",
    "msw": "^2.12.4",
    "nyc": "^17.1.0",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.96.0",
    "scss": "^0.2.4",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.9.3",
    "vite": "7.2.7",
    "vite-plugin-istanbul": "^7.2.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.8",
    "vuetify": "^3.11.3"
  }
}
